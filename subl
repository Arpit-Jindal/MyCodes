#include<bits/stdc++.h>
using namespace std;
int temp;

char check(char s[],int l,int start)
{
    temp=0;
    char x;
    for(int i=start;i<l;i=i+4)
    {
        if(s[i]=='!')   temp++;
        else    x=s[i];

    }
    return x;
}
int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    char s[101];
    int kr=0,ky=0,kb=0,kg=0;
    cin>>s;
    int l=strlen(s);
    for(int i=0;i<4;i++)
    {
        char tempchar = check(s,l,i);
        if(tempchar=='R')
            kr=temp;
        else if(tempchar=='Y')
            ky=temp;
        else if(tempchar=='B')
            kb=temp;
        else
            kg=temp;

    }
    cout<<kr<<' '<<kb<<' '<<ky<<' '<<kg;
    return 0;
}
